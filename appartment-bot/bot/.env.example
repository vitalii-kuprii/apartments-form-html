# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_WEBHOOK_URL=
WEBAPP_URL=https://your-webapp-url.vercel.app
ADMIN_IDS=your_telegram_user_id

# Database (PostgreSQL)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/apartment_bot

# Redis (for BullMQ job queues)
REDIS_URL=redis://localhost:6379

# Job Processing
FETCH_CONCURRENCY=5

# DOM RIA API
DOMRIA_API_KEY=your_api_key_here

# Application
NODE_ENV=development
PORT=3000

# Grafana Cloud (OpenTelemetry - optional)
OTEL_EXPORTER_OTLP_ENDPOINT=https://otlp-gateway-prod-eu-west-2.grafana.net/otlp
GRAFANA_CLOUD_USER=your_user_id
GRAFANA_CLOUD_TOKEN=your_token_here

# Grafana Cloud Loki (for logs - optional)
GRAFANA_LOKI_URL=https://logs-prod-012.grafana.net
GRAFANA_LOKI_USER=your_loki_user_id
